        "@babel/preset-env":"^7.1.0",
        "@babel/preset-react":"^7.0.0",
        "babel-loader":"^8.0.2",
        "concurrently":"^5.3.0",
        "cross-env":"^7.0.3",
        "css-loader":"^1.0.0",
        "nodemon":"^2.0.7",
        "sass":"^1.32.6",
